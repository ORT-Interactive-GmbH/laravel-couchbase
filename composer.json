{
  "name": "ort-interactive-gmbh/laravel-couchbase",
  "description": "A Couchbase based Eloquent model and Query builder for Laravel",
  "keywords": [
    "laravel",
    "eloquent",
    "couchbase",
    "database",
    "model"
  ],
  "homepage": "https://github.com/ORT-Interactive-GmbH/laravel-couchbase",
  "authors": [
  ],
  "license": "MIT",
  "require": {
    "php": "^7.2",
    "illuminate/support": "^7",
    "illuminate/container": "^7",
    "illuminate/database": "^7",
    "illuminate/events": "^7",
    "ext-couchbase": ">=2.4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5",
    "orchestra/testbench": "^5",
    "mockery/mockery": "^1.3.1",
    "vlucas/phpdotenv": ">=4.0.0"
  },
  "autoload": {
    "psr-4": {
      "": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php",
      "tests/models",
      "tests/seeds"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "ORT\\Interactive\\Couchbase\\CouchbaseServiceProvider"
      ]
    }
  }
}
